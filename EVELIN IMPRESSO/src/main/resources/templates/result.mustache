<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>EVELIN</title>

    <!-- Bootstrap Core CSS -->
    <link href=" css/bootstrap.min.css" rel="stylesheet">


    <!-- Datatables -->
    <link href=" css/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href=" css/main.css" rel="stylesheet">
    <!-- Tag Input-->
    <link href=" css/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Dialog-->
    <link href=" css/bootstrap-dialog.css" rel="stylesheet">
    <!-- Font Awsome-->
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet">

            <style>
                .inner
                {padding:5px;}
            </style>
</head>

<body>
<!--Navbar-->
{{>navbar}}
<!--Cache itemes-->
{{#cache}}
    <span class="cache-items">
                        <input type="hidden" class="name-label" value="{{label}}"/>
                        <input type="hidden" class="name-type" value="{{type}}"/>
                        <input type="hidden" class="name-id" value="{{node_id}}"/>
                    </span>
{{/cache}}
{{#target}}
    <input id="hidden_target" type="hidden" class="name-type" value="{{target}}"/>
{{/target}}
<!-- hidden field to save the query from the page before -->
<input type="hidden" id="query" value="{{query}}"></input>
<!-- Loading image  -->
<div id="intro" class="text-center" >
    <!-- Loading image  -->
    <img id="loading-image" style="margin: auto;" src="js/img/balls.gif" class="hidden"></img>
</div>
<!-- Table -->
<section id="table" class="content hidden">
    <div class="container">
        <div class="row">
            <div class=" col-lg-12 text-center">
                <div class="x_content">
                    <!-- Messages  -->
                    <div id="message" class="alert alert-danger hidden" role="alert">{{message}}</div>
                    <!--Date Filters-->
                    <div id="date_filter" class="container hidden">
                        <p>Date precision:</p>
                        <form>
                            <label class="radio-inline">
                                <input type="radio" value="year" name="optradio">Year
                            </label>
                            <label class="radio-inline">
                                <input type="radio" value="month" name="optradio">Month
                            </label>
                            <label class="radio-inline">
                                <input type="radio" value="day" name="optradio">Day
                            </label>
                        </form>
                        <input type="hidden" id="isDate" value="false"></input>
                    </div>


                    <table id="datatable" class="table table-striped table-bordered">
                        <thead>
                        <tr>
                            <th id="header" ></th>
                            <th>Score</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container -->
</section>
<!--footer-->
{{>footer}}
<!-- jQuery -->
<script src=" js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src=" js/bootstrap.min.js"></script>
<!-- Datatables -->
<script src=" css/datatables.net/js/jquery.dataTables.min.js"></script>
<script src=" css/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>

<!-- JS file -->
<!-- typeahed and tags input  -->
<script src=" js/bootstrap-tagsinput.js"></script>
<script src=" js/bootstrap-typeahead.js"></script>
<!-- modal -->
<script src=" js/bootstrap-dialog.js"></script>
<!-- fingerprint of the browser  -->
<script src=" js/prog.js"></script>
<!-- javascript for the page  -->
<script src=" js/result_page.js"></script>

<!-- fingerprint of the browser  -->
<script src=" js/fingerprint.js"></script>

<script>

</script>


</body>

</html>

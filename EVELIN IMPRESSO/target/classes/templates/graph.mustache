<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>EVELIN</title>
    <!-- Bootstrap Core CSS -->
    <link href=" css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href=" css/main.css" rel="stylesheet">
    <!-- Datatables -->
    <link href=" css/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <!-- Tag Input-->
    <link href=" css/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Dialog-->
    <link href=" css/bootstrap-dialog.css" rel="stylesheet">

</head>

<body>
<!--Navbar-->
{{>navbar}}

<!--Cache itemes-->
{{#cache}}
<span class="cache-items">
                        <input type="hidden" class="name-label" value="{{label}}"/>
                        <input type="hidden" class="name-type" value="{{type}}"/>
                        <input type="hidden" class="name-id" value="{{node_id}}"/>
                    </span>
{{/cache}}
<!-- Graph section  -->
<section id="graph" class="content " style="padding: 0px;">
    <div class="row">
        <div class="col-lg-10 col-lg-offset-1 text-center">
            <!-- Messages  -->
            <div id="message" class="alert alert-danger hidden" role="alert">{{message}}</div>

        </div>
    </div>

    <!-- hidden field to save the query from the page before -->
<input type="hidden" id="query" value="{{query}}"></input>

<!-- Graph Svg-->
<div id="intro" class="text-center" >
    <!-- Loading image  -->
    <img id="loading-image" style="margin: auto;" src="js/img/balls.gif" class="hidden"></img>
</div>
<div class="row">
    <div id ="graph-container" class="col-lg-10">
        <svg ></svg>
    </div>
</div>

</section>

<!--footer-->
{{>footer}}

<!-- jQuery -->
<script src=" js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src=" js/bootstrap.min.js"></script>

<!-- JS file -->
<!-- graph  -->
<script src=" js/d3.min.js"></script>
<script src=" js/graph.js"></script>

<script type="text/javascript" src="dist/vis.js"></script>
<link href="dist/vis-network.min.css" rel="stylesheet" type="text/css">
<!-- typeahed and tags input  -->
<script src=" js/bootstrap-tagsinput.js"></script>
<script src=" js/bootstrap-typeahead.js"></script>
<!-- fingerprint of the browser  -->
<script src=" js/fingerprint.js"></script>
<!-- modal -->
<script src=" js/bootstrap-dialog.js"></script>
<!-- custom script for the navbar  -->
<script src=" js/prog.js"></script>
<!-- javascript for the page  -->
<script src=" js/graph_page.js"></script>

</body>

</html>
